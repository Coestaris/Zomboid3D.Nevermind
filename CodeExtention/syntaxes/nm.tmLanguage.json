{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Nevermind",
	"patterns": 
	[
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		}
	],
	"repository": 
	{
		"keywords": 
		{
			"patterns": 
			[
				{
					"name": "keyword.control.nm",
					"match": "\\b(var|void|import|function)\\b"
				},
				{
					"name": "keyword.type.nm",
					"match": "\\b(integer|int)\\b"
				},
				{
					"name": "keyword.operator.nm",
					"match": "\\b(integer|int)\\b"
				},
				{
					"name": "keyword.operator.nm",
					"match": "\\b(integer|int)\\b"
				}
			]
		},
		"strings": 
		{
			"name": "string.quoted.double.nm",
			"begin": "\"",
			"end": "\"",
			"patterns": 
			[
				{
					"name": "constant.character.escape.char.nm",
					"match": "\\\\."
				},
				{
					"name": "constant.character.escape.hex.nm",
					"match": "\\\\x\\d{2}"
				}
			]
		}
	},
	"scopeName": "source.nm"
}